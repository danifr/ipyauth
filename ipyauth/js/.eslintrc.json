{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "prettier",
        "html"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "no-console": "off",
        "func-names": "off",
        "no-undef": "off",
        "camelcase": "off"
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}